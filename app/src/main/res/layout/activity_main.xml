<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">
    
     <TextView
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="55dp"
        android:textStyle="bold"
        android:textSize="55dp"
        android:text="StudyIntent"/>

    <Button
        android:id="@+id/btn_intenbiasa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Intent Biasa"/>

    <Button
        android:id="@+id/btn_inten2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Intent ke 2"/>

    <Button
        android:id="@+id/btn_inten3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Itent ke 3"/>

    <Button
        android:id="@+id/btn_inten4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Intent ke 4"/>




</LinearLayout>
